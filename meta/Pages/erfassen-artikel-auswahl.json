            "color": "#3a474d",
            "left": "0",
            "paddingRight": "10px",
            "position": "fixed",
            "right": "0",
            "top": "0",
            "zIndex": "3000"
          },
          "type": "row"
        },
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "allowPaging": false,
                  "count": "${rstArtikelCount}",
                  "data": "${rstArtikel}",
                  "defaultRowText": "Template for index",
                  "emptyText": "Artikel nicht gefunden!",
                  "events": {
                    "loadData": [
                      {
                        "name": "dbOptimo.getInventurArtikels",
                        "parameters": [
                          {
                            "name": "$top",
                            "value": "50"
                          },
                          {
                            "name": "$filter",
                            "value": [
                              {
                                "value": "InventurID eq ${parameters.InventurID}"
                              },
                              {
                                "filters": [
                                  {
                                    "value": "contains(tolower(Artikelnummer),tolower('${this.textboxArtikelSuchen.value || ''}')) OR contains(tolower(Beschreibung),tolower('${this.textboxArtikelSuchen.value || ''}'))"
                                  }
                                ],
                                "operator": "or"
                              }
                            ]
                          },
                          {
                            "name": "$orderby",
                            "value": [
                              {
                                "value": "Artikelnummer"
                              }
                            ]
                          }
                        ],
                        "then": [
                          {
                            "name": "rstArtikel",
                            "type": "set",
                            "value": "${result.value}"
                          },
                          {
                            "name": "rstArtikelCount",
                            "type": "set",
                            "value": "event.top != null && event.skip != null ? result['@odata.count'] : ${result.value.length}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "name": "datalistArtikel",
                  "pageSize": 14,
                  "style": {
                    "border": "1px solid #ffffff"
                  },
                  "template": [
                    {
                      "attributes": [
                        {
                          "name": "(click)",
                          "value": "onClickNavigateBack(data)"
                        }
                      ],
                      "components": [
                        {
                          "components": [
                            {
                              "class": "col-10",
                              "components": [
                                {
                                  "name": "heading0",
                                  "size": "H5",
                                  "style": {
                                    "color": "#6264A7",
                                    "fontWeight": "normal",
                                    "marginBottom": "0",
                                    "marginLeft": "0",
                                    "marginRight": "0",
                                    "marginTop": "0",
                                    "textAlign": "left",
                                    "verticalAlign": "baseline"
                                  },
                                  "text": "${data.Artikelnummer}",
                                  "type": "heading"
                                },
                                {
                                  "name": "label3",
                                  "style": {
                                    "color": "#3a474d",
                                    "fontSize": "18px",
                                    "fontStyle": "normal",
                                    "fontWeight": "normal",
                                    "lineHeight": "20px",
                                    "width": "100%"
                                  },
                                  "text": "${data.Beschreibung}",
                                  "type": "label",
                                  "visible": true
                                },
                                {
                                  "name": "label0",
                                  "style": {
                                    "color": "#3a474d",
                                    "fontSize": "16px",
                                    "fontStyle": "normal",
                                    "fontWeight": "normal",
                                    "lineHeight": "18px",
                                    "width": "100%"
                                  },
                                  "text": "${data.Beschreibung2}",
                                  "type": "label",
                                  "visible": "${data.Beschreibung2 != null}"
                                },
                                {
                                  "name": "label1",
                                  "style": {
                                    "color": "#3a474d",
                                    "fontSize": "16px",
                                    "fontStyle": "normal",
                                    "fontWeight": "normal",
                                    "lineHeight": "18px",
                                    "width": "100%"
                                  },
                                  "text": "${data.StdKreditorName}",
                                  "type": "label",
                                  "visible": "${data.StdKreditorName != null}"
                                }
                              ],
