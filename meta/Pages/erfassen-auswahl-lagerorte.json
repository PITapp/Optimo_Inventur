{
  "access": [
    "Everybody"
  ],
  "components": [
    {
      "components": [
        {
          "allowPaging": false,
          "count": "${rstLagerorteCount}",
          "data": "${rstLagerorte}",
          "defaultRowText": "Template for index",
          "emptyText": "No records found.",
          "events": {
            "loadData": [
              {
                "name": "dbOptimo.getInventurBases",
                "parameters": [
                  {
                    "name": "$orderby",
                    "value": "LagerortNummer"
                  }
                ],
                "then": [
                  {
                    "name": "rstLagerorte",
                    "type": "set",
                    "value": "${result.value}"
                  },
                  {
                    "name": "rstLagerorteCount",
                    "type": "set",
                    "value": "event.top != null && event.skip != null ? result['@odata.count'] : ${result.value.length}"
                  }
                ],
                "type": "invoke"
              }
            ]
          },
          "name": "datalistLagerorte",
          "pageSize": 3,
          "style": {
            "marginBottom": "10px",
            "marginTop": "-11px"
          },
          "template": [
            {
              "name": "row0",
              "style": {
                "backgroundColor": "#e9e9e9",
                "height": "10px"
              },
              "type": "row"
            },
            {
              "attributes": [
                {
                  "name": "(click)",
                  "value": "onClickStartErfassen(data)"
                }
              ],
              "components": [
                {
                  "components": [
                    {
                      "class": "col-2 align-items-center d-flex",
                      "components": [
                        {
                          "name": "heading0",
                          "size": "H5",
                          "style": {
                            "color": "#3a474d",
                            "fontSize": "30px",
                            "fontWeight": "normal",
                            "textAlign": "left",
                            "verticalAlign": "baseline"
                          },
                          "text": "${data.LagerortNummer}",
                          "type": "heading"
                        }
                      ],
                      "name": "col21",
                      "style": {
                        "fontSize": "16px",
                        "lineHeight": "16px",
                        "textAlign": "right"
                      },
                      "type": "col"
                    },
                    {
                      "class": "col-8",
                      "components": [
                        {
                          "name": "heading4",
                          "size": "H5",
                          "style": {
                            "color": "#3a474d",
                            "fontSize": "18px",
                            "fontWeight": "normal",
                            "lineHeight": "20px",
                            "marginBottom": "0"
                          },
                          "text": "${data.LagerortTitel}",
                          "type": "heading"
                        },
                        {
                          "name": "label3",
                          "style": {
                            "color": "#598087",
                            "fontSize": "12px",
                            "fontStyle": "normal",
                            "fontWeight": "normal",
                            "width": "100%"
                          },
                          "text": "Status: offen",
                          "type": "label"
                        }
                      ],
                      "name": "col1",
                      "style": {
                        "paddingLeft": "10px",
                        "paddingRight": "0px",
                        "verticalAlign": "baseline"
                      },
                      "type": "col"
                    },
                    {
                      "class": "align-items-center d-flex justify-content-end col-2",
                      "components": [
                        {
                          "icon": "keyboard_arrow_right",
                          "name": "icon4",
                          "style": {
                            "color": "#a71330",
                            "fontSize": "32px",
                            "fontWeight": "normal"
                          },
                          "type": "icon"
                        }
                      ],
                      "name": "col15",
                      "style": {
                        "textAlign": "right"
                      },
                      "type": "col"
                    }
                  ],
                  "name": "row1",
                  "type": "row"
                }
              ],
              "name": "card0",
              "style": {
                "border": "1px solid #ffffff",
                "marginLeft": "-1px",
                "marginRight": "-1px",
                "paddingBottom": "10px",
                "paddingLeft": "15px",
                "paddingRight": "5px",
                "paddingTop": "10px"
              },
              "type": "card",
              "visible": true
            }
          ],
          "type": "datalist",
          "wrapItems": false
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "code": "this.datalistLagerorte.load();",
        "type": "execute"
      }
    ]
  },
  "layout": "InventurErfassen",
  "name": "ErfassenAuswahlLagerorte"
}