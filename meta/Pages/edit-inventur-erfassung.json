{
  "components": [
    {
      "type": "content",
      "container": "main",
      "components": [
        {
          "type": "heading",
          "name": "pageTitle",
          "size": "H1",
          "text": "Edit Inventur Erfassung"
        },
        {
          "type": "row",
          "components": [
            {
              "type": "col",
              "class": "col-md-12",
              "components": [
                {
                  "data": "${inventurerfassung}",
                  "events": {
                    "cancel": [
                      {
                        "type": "back"
                      }
                    ],
                    "submit": [
                      {
                        "error": [
                          {
                            "detail": "Unable to update InventurErfassung",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "dbOptimo.updateInventurErfassung",
                        "parameters": [
                          {
                            "name": "ErfassungID",
                            "value": "${parameters.ErfassungID}"
                          },
                          {
                            "name": "InventurErfassung",
                            "value": "${event}"
                          }
                        ],
                        "then": [
                          {
                            "type": "back"
                          }
                        ],
                        "type": "invoke"
                      }
                    ]
                  },
                  "fields": [
                    {
                      "data": "${getBenutzersResult}",
                      "property": "BenutzerID",
                      "required": true,
                      "textProperty": "AspNetUsers_Id",
                      "title": "Benutzer",
                      "type": "lookup",
                      "valueProperty": "BenutzerID",
                      "placeholder": "Choose Benutzer"
                    },
                    {
                      "data": "${getInventurArtikelsResult}",
                      "property": "ArtikelID",
                      "required": true,
                      "textProperty": "Artikelnummer",
                      "title": "Inventur Artikel",
                      "type": "lookup",
                      "valueProperty": "ArtikelID",
                      "placeholder": "Choose InventurArtikel"
                    },
                    {
                      "property": "ErfasstAm",
                      "required": true,
                      "title": "Erfasst Am",
                      "type": "date"
                    },
                    {
                      "property": "ErfasstAnzahl",
                      "required": true,
                      "title": "Erfasst Anzahl",
                      "type": "integer"
                    },
                    {
                      "property": "Notiz",
                      "required": false,
                      "title": "Notiz",
                      "type": "string",
                      "maxLength": 65535
                    }
                  ],
                  "name": "form0",
                  "type": "form",
                  "visible": "inventurerfassung"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "events": {
    "load": [
      {
        "name": "dbOptimo.getInventurErfassungByErfassungID",
        "parameters": [
          {
            "name": "ErfassungID",
            "value": "${parameters.ErfassungID}"
          }
        ],
        "then": [
          {
            "name": "inventurerfassung",
            "type": "set",
            "value": "${result}"
          }
        ],
        "error": [],
        "type": "invoke"
      },
      {
        "name": "dbOptimo.getBenutzers",
        "parameters": [],
        "then": [
          {
            "name": "getBenutzersResult",
            "type": "set",
            "value": "${result.value}"
          }
        ],
        "type": "invoke"
      },
      {
        "name": "dbOptimo.getInventurArtikels",
        "parameters": [],
        "then": [
          {
            "name": "getInventurArtikelsResult",
            "type": "set",
            "value": "${result.value}"
          }
        ],
        "type": "invoke"
      }
    ]
  },
  "name": "Edit Inventur Erfassung",
  "layout": "InventurStart"
}