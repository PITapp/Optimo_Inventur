{
  "components": [
    {
      "components": [
        {
          "name": "pageTitle",
          "size": "H1",
          "text": "InventurErfassung",
          "type": "heading"
        },
        {
          "components": [
            {
              "class": "col-md-12",
              "components": [
                {
                  "addText": "Add",
                  "allowAdd": false,
                  "allowDelete": true,
                  "allowFiltering": false,
                  "allowMultiSorting": false,
                  "allowPaging": true,
                  "allowSorting": true,
                  "applyFilterText": "Apply",
                  "clearFilterText": "Clear",
                  "columns": [
                    {
                      "format": "date-time",
                      "formatString": "medium",
                      "property": "ErfasstAm",
                      "title": "Erfasst Am",
                      "type": "string"
                    },
                    {
                      "property": "ErfasstAnzahl",
                      "title": "Erfasst Anzahl",
                      "type": "integer"
                    }
                  ],
                  "confirmDelete": true,
                  "confirmDeleteCancelText": "Cancel",
                  "confirmDeleteOkText": "Ok",
                  "confirmDeleteText": "Are you sure you want to delete this record?",
                  "confirmDeleteTitle": "Confirmation",
                  "containsText": "Contains",
                  "count": "${getInventurErfassungsCount}",
                  "data": "${getInventurErfassungsResult}",
                  "emptyText": "No records to display.",
                  "endsWithText": "Ends with",
                  "equalsText": "Equals",
                  "events": {
                    "add": [
                      {
                        "path": "add-inventur-erfassung",
                        "type": "navigate"
                      }
                    ],
                    "delete": [
                      {
                        "error": [
                          {
                            "detail": "Unable to delete InventurErfassung",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "dbOptimo.deleteInventurErfassung",
                        "parameters": [
                          {
                            "name": "ErfassungID",
                            "value": "${event.ErfassungID}"
                          }
                        ],
                        "then": [
                          {
                            "detail": "InventurErfassung deleted!",
                            "severity": "success",
                            "summary": "Success",
                            "type": "notify"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "loadData": [
                      {
                        "error": [
                          {
                            "detail": "Unable to load InventurErfassungs",
                            "severity": "error",
                            "summary": "Error",
                            "type": "notify"
                          }
                        ],
                        "name": "dbOptimo.getInventurErfassungs",
                        "parameters": [
                          {
                            "name": "$top",
                            "value": "event.top"
                          },
                          {
                            "name": "$skip",
                            "value": "event.skip"
                          },
                          {
                            "name": "$count",
                            "value": "event.top != null && event.skip != null"
                          },
                          {
                            "name": "$filter",
                            "value": "${event.filter}"
                          },
                          {
                            "name": "$orderby",
                            "value": "${event.orderby}"
                          },
                          {
                            "name": "$expand",
                            "value": "InventurArtikel"
                          }
                        ],
                        "then": [
                          {
                            "name": "getInventurErfassungsResult",
                            "type": "set",
                            "value": "${result.value}"
                          },
                          {
                            "name": "getInventurErfassungsCount",
                            "type": "set",
                            "value": "event.top != null && event.skip != null ? result['@odata.count'] : ${result.value.length}"
                          }
                        ],
                        "type": "invoke"
                      }
                    ],
                    "rowSelect": [
                      {
                        "parameters": [
                          {
                            "name": "ErfassungID",
                            "value": "${event.ErfassungID}"
                          }
                        ],
                        "path": "edit-inventur-erfassung",
                        "type": "navigate"
                      }
                    ]
                  },
                  "greaterThanOrEqualsText": "Greater than or equals",
                  "greaterThanText": "Greater than",
                  "lessThanOrEqualsText": "Less than or equals",
                  "lessThanText": "Less than",
                  "name": "grid0",
                  "notEqualsText": "Not equals",
                  "startsWithText": "Starts with",
                  "title": "",
                  "type": "grid"
                }
              ],
              "type": "col"
            }
          ],
          "type": "row"
        }
      ],
      "container": "main",
      "type": "content"
    }
  ],
  "events": {
    "load": [
      {
        "code": "this.grid0.load();",
        "type": "execute"
      }
    ]
  },
  "layout": "InventurStart",
  "name": "InventurErfassung"
}