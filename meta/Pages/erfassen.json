  "events": {
    "load": [
      {
        "code": "window.scroll(0,0);",
        "type": "execute"
      },
      {
        "name": "dbOptimo.getInventurBases",
        "parameters": [
          {
            "name": "$filter",
            "value": "InventurID eq ${parameters.InventurID}"
          }
        ],
        "then": [
          {
            "name": "dsoLagerort",
            "type": "set",
            "value": "${result.value[0]}"
          }
        ],
        "type": "invoke"
      },
      {
        "code": "this.datalistLagerorte.load();",
        "type": "execute"
      },
      {
        "name": "onClickAuswahlLagerorte",
        "type": "set",
        "value": "() => {\n    this.buttonOpenDialogAuswahlLagerorteClick('');\n}"
      },
